<div class="container">
  <h1>Wspólnota Żywego Różańca Św. Kalendarz na rok 2021 <img src="/image/rose.jpg" classs = "rose" alt="rose"></h1>
  <table class="table">
    <tbody>
      <tr class="red tablegroup">
        <td >Nr Róży. wezwanie zelator</td>
        <% heders.forEach(function(heder){ %>
        <td width="65px"><%=heder%></td>
        <% });%>
      </tr>
       
       <%for(let x = 0; rosaryHarmonogram[x] !== 'end';x++){%>
        
          <%-rosaryHarmonogram[x]%>
        
        <%}%>

        <tr class="red tablegroup">
          <td >Pierwsza niedziela m-ca 2021</td>
          <% heders.forEach(function(heder){ %>
          <td><%=heder%></td>
          <% });%>
        </tr>
      
    </tbody>
  </table>
  <form action="/harmonogram" method="post">
    <input type="submit" value="Zsynchronizuj">
  </form>
</div>

<style type="text/css" media="print">
  @page { 
      size: landscape;
  }

</style>

